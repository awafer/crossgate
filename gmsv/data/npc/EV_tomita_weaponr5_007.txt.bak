###　나이프직인이고 나이프작성 스킬레벨이 ５이상

block JobAncestry == 260 && SkillLevel_206 >= 5


#　빨간 휘슬을 소지
	If Item_607950 >= 1
		EndWindow 8700001
		EndBlock


#　부지깽이와 알마이트（２０개）를 소지
	else

		If Item_607947 >= 1 && Item_9708 >= 20
			Window 8700002,NEXT
			Window 8700003,OK

				Window_OK Item_607947 >= 1 && Item_9708 >= 20 && ItemSpace >= 1
					KillItemAll 607947
					KillItem 9708,20
					GiveItemEX 607950,1,"\n\n　소지품을 정리해. 휘슬을 줄 수 없어."
					EndBlock

				Window_OK Item_607947 >= 1 && Item_9708 >= 20
					EndWindow 8700004
					EndBlock

				Window_OK Item_607947 >= 1
					EndWindow 8700005
					EndBlock

				Window_OK
					EndWindow 8700006
					EndBlock


#　부지깽이는 소지하고 있지만, 알마이트가 부족
		else

			If Item_607947 >= 1
				EndWindow 8700007
				EndBlock


#　주문서를 소지
			else

				If Item_607942 >= 1
					EndWindow 8700008
					EndBlock


#　상기 아이템을 소지하고 있지 않음
				else
					Window 8700009,YES,NO

						Window_YES
							Window 8700010,NEXT
							Window 8700011,OK

								Window_OK ItemSpace >= 1
									GiveItemEX 607942,1,"\n\n　소지품을 정리해. 주문서를 줄 수 없어."
									EndBlock

								Window_OK
									EndWindow 8700012
									EndBlock

						Window_NO
							EndWindow 8700013
							EndBlock


				endif

			endif

		endif

	endif



