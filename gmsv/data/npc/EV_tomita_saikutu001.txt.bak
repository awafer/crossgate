			Window 8260007,OK

				Window_OK Item_607905 >= 1
					EndWindow 8260008

				Window_OK
					GiveItemEX 607905,1,"\n\n　소지품이 가득 차서 드릴 수가 없습니다.\n　정리해주십시오."

		Window_YES
			EndWindow 8260009

		Window_NO
			EndWindow 8260010



