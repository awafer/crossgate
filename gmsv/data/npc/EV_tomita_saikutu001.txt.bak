block Item_607905 >= 2
	EndWindow 8260001



###　광부이다

#　엔트리 증 소지
block JobAncestry == 470 && Item_607905 == 1

	If StampCount_607905 >= 2
		EndWindow 8260002
		EndBlock

	else
		If StampCount_607905 == 1
			EndWindow 8260003
			EndBlock

		else
			EndWindow 8260004
			EndBlock

		endif

	endif


#　엔트리 증소지하지 않음 
